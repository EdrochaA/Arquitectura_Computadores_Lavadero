;			MÁQUINA DE ESTADOS
;#########################################################################

MAQ_ESTADOS:
	MOV A, ESTADO
	RL A
	MOV DPTR, #LISTA_EST
	JMP @A+DPTR

LISTA_EST:
	AJMP ESPERA_COCHE;   Estado0
	AJMP COCHE_PUESTO;   Estado1
	AJMP ALARMA;         Estado2
	AJMP PONER_PUENTE;   Estado3
	AJMP ENJABONADO;     Estado4
	AJMP LIMPIAR_ATRAS;  Estado5
	AJMP PONER_RODILLOS; Estado6
	AJMP RODILLOS;       Estado7
	AJMP LIMPIAR_DELANTE;Estado8
	AJMP ACLARADO;       Estado9
	AJMP SECADO;         Estado10
	AJMP RETIRAR_COCHE;  Estado11
	
GEN_EVE:

    ; VERIFICACION DE LOS BITS DEL PUERTO 0
    MOV A, ACT0             
    MOV ANT0, A              ; Dar valores a ACT y ANT
    MOV ACT0, P0

    MOV A, ACT0              
    XRL A, ANT0              ; XOR con el valor anterior para detectar cambios
    JZ PUERTO1_GV           
    MOV B, ANT0


    ; Verificación para bit 0.2 (S_PLAT)
    JNB ACC.2, S_FICHA_GV   
    JNB B.2, ASCENDENTE_SPLAT   ; Si ANT.2 era 0 y cambió a 1, es un flanco ascendente
    JB B.2, DESCENDENTE_SPLAT   ; Si ANT.2 era 1 y cambió a 0, es un flanco descendente

    ASCENDENTE_SPLAT:
    SETB EV2    ; EVENTO 2
    SJMP S_FICHA_GV

    DESCENDENTE_SPLAT:
    SETB EV3   ; EVENTO 3


    ; Verificación para bit 0.4 (S_FICHA)
    S_FICHA_GV:        
  
    JNB ACC.4, P_START_GV    
    JNB B.4, ASCENDENTE_SFICHA ; Si ANT.2 era 0 y cambió a 1, es un flanco ascendente

    ASCENDENTE_SFICHA:
    SETB EV5    ; EVENTO 5
    SJMP P_START_GV


    ; Verificación para bit 0.5 (P_START)
    P_START_GV:    
 
    JNB ACC.5, PUERTO1_GV     
    JNB B.5, ASCENDENTE_PSTART ; Si ANT.2 era 0 y cambió a 1, es un flanco ascendente

    ASCENDENTE_PSTART:
    SETB EV6    ; EVENTO 6
    SJMP PUERTO1_GV



    ; VERIFICACION DE LOS BITS DEL PUERTO 1
    PUERTO1_GV:    	
    MOV A, ACT1             
    MOV ANT1, A              ; Dar valores a ACT y ANT
    MOV ACT1, P1

    MOV A, ACT1              
    XRL A, ANT1              ; XOR con el valor anterior para detectar cambios
    JZ PUERTO2_GV           
    MOV B, ANT1

    ; Verificación para bit 1.2 (FC_RH_TOP)

    JNB ACC.2, FC_RV_BORDE     
    JBC B.2, ASCENDENTE_FC_RH_TOP_GV ; Si ANT.2 era 0 y cambió a 1, es un flanco ascendente

    ASCENDENTE_FC_RH_TOP_GV:
    SETB EV19    ; EVENTO 19
    SJMP FC_RV_BORDE 

    ; Verificación para bit 1.5 (FC_RV_BORDE)
    FC_RV_BORDE:

    JNB ACC.5, PUERTO2_GV     
    JBC B.5, ASCENDENTE_FC_RV_BORDE_GV ; Si ANT.2 era 0 y cambió a 1, es un flanco ascendente

    ASCENDENTE_FC_RV_BORDE_GV:
    SETB EV16    ; EVENTO 16
    SJMP PUERTO2_GV    

    ; VERIFICACION DE LOS BITS DEL PUERTO 2
    PUERTO2_GV:
            
    MOV A, ACT2             
    MOV ANT2, A              ; Dar valores a ACT y ANT
    MOV ACT2, P2

    MOV A, ACT2              
    XRL A, ANT2              ; XOR con el valor anterior para detectar cambios
    JZ FIN_PUERTOS           
    MOV B, ANT2

    ; Verificación para bit 2.3 (S_CAR)  
    JNB ACC.3, FC_RV_CENTRO_GV     
    JNB B.3, ASCENDENTE_SCAR   ; Si ANT.2 era 0 y cambió a 1, es un flanco ascendente
    JC DESCENDENTE_SCAR        ; Si ANT.2 era 1 y cambió a 0, es un flanco descendente

    ASCENDENTE_SCAR:
    SETB EV8    ; EVENTO 8
    SJMP FC_RV_CENTRO_GV

    DESCENDENTE_SCAR:
    SETB EV9    ; EVENTO 9


    ; Verificación para bit 2.4 (FC_RV_CENTRO)
    FC_RV_CENTRO_GV:
  
    JNB ACC.4, FIN_PUERTOS     
    JNB B.4, ASCENDENTE_FC_RV_CENTRO_GV ; Si ANT.2 era 0 y cambió a 1, es un flanco ascendente

    ASCENDENTE_FC_RV_CENTRO_GV:
    SETB EV17    ; EVENTO 17
    SJMP FIN_PUERTOS


    FIN_PUERTOS:

    ; SELECTOR DE PRIORIDAD DE LOS FLAGS DE EVENTOS

    JNB EV1, EV2_GV       ; EVENTO 1 ACTIVADO
    MOV EVENTO, #0x01
    CLR EV1
    LJMP FIN_GV

EV2_GV:
    JNB EV2, EV3_GV       ; EVENTO 2 ACTIVADO
    MOV EVENTO, #0x02
    CLR EV2
    LJMP FIN_GV

EV3_GV:
    JNB EV3, EV4_GV       ; EVENTO 3 ACTIVADO
    MOV EVENTO, #0x03
    CLR EV3
    LJMP FIN_GV

EV4_GV:
    JNB EV4, EV5_GV       ; EVENTO 4 ACTIVADO
    MOV EVENTO, #0x04
